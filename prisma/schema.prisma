generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Board {
  id     BigInt  @id @default(autoincrement())
  title  String  @db.VarChar
  disc   String? @db.VarChar
  status String? @default("PUBLIC") @db.VarChar
}
